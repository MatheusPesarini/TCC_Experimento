{
  "name": "tcc-experimento",
  "version": "1.0.0",
  "description": "Ambiente de testes para análise de código gerado por LLMs",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["typescript", "express", "testing", "code-analysis"],
  "author": "Matheus Pesarini",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "@types/express": "^4.17.17",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.13",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4",
    "@types/node": "^20.3.1",
    "ts-node": "^10.9.1",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.2",
    "ts-jest": "^29.1.0",
    "eslint": "^8.42.0",
    "@typescript-eslint/parser": "^5.59.11",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "eslint-config-google": "^0.14.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12"
  },
  "engines": {
    "node": ">=20.17.1"
  }
}
